{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=5.5.9",
        "guzzlehttp/guzzle": "~5.3|~6.0",
        "laravel/framework": "5.3.*",
        "dingo/api": "dev-master",
        "tymon/jwt-auth": "1.0.*@dev",
        "ramsey/uuid": "^3.4",
        "nategood/httpful": "^0.2.20",
        "hoy/pushwoosh": "^2.1",
        "league/fractal": "^0.13.0",
        "tucker-eric/eloquentfilter": "^0.2.0",
        "laravel/socialite": "^2.0",
        "laracasts/utilities": "~2.0",
        "cviebrock/eloquent-sluggable": "4.0.2",
        "cmgmyr/messenger": "~2.0",
        "vinkla/hashids": "^2.3",
        "barryvdh/laravel-debugbar": "^2.2",
        "barryvdh/laravel-cors": "^0.8.0",
        "get-stream/stream-laravel": "~2.2.6",
        "zizaco/entrust": "5.2.x-dev",
        "socialiteproviders/twitter": "^1.0",
        "grimthorr/laravel-user-settings": "^2.1",
        "jrean/laravel-user-verification": "^3.0",
        "predis/predis": "^1.1",
        "nutsweb/laravel-prerender": "^2.0",
        "webup/laravel-sendinblue": "^1.0",
        "payum/payum-laravel-package": "^1.0",
        "payum/stripe": "^1.3",
        "payum/paypal-pro-checkout-nvp": "^1.3",
        "berkayk/onesignal-laravel": "^0.9.3",
        "yajra/laravel-datatables-oracle": "~6.0",
        "intercom/intercom-php": "^3.0",
        "laravelista/ekko": "^1.3",
        "artesaos/seotools": "^0.10.0",
        "darkin1/intercom": "^1.0",
        "roumen/sitemap": "^2.6",
        "pusher/pusher-php-server": "^2.6"
    },
    "require-dev": {
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "~4.0",
        "phpspec/phpspec": "~2.1",
        "drewm/mailchimp-api": "dev-api-v3",
        "symfony/dom-crawler": "~3.0",
        "symfony/css-selector": "~3.0",
        "laracasts/generators": "^1.1"
    },
    "autoload": {
        "classmap": [
            "database"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/TestCase.php"
        ]
    },
    "scripts": {
        "post-install-cmd": [
            "php artisan clear-compiled",
            "php artisan optimize"
        ],
        "pre-update-cmd": [
            "php artisan clear-compiled"
        ],
        "post-update-cmd": [
            "php artisan optimize"
        ],
        "post-root-package-install": [
            "php -r \"copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "php artisan key:generate"
        ]
    },
    "config": {
        "preferred-install": "dist"
    },
    "minimum-stability": "dev",
    "prefer-stable" : true
}
